	<div class="container">
		<h1>Ajouter</h1>
		<form action="/add" method="POST">
			<div class="form-group">
				<label for="title">Nom</label>
				<input 
					type="text" 
					class="form-control" 
					id="title" 
					name="name"
					placeholder="Nom Client..."
				/>
			</div>
			
			<label for="country">Cin</label>
				<input 
					type="text" 
					class="form-control" 
					id="local" 
					name="Cin"
					placeholder="Votre Cin"
				/>
				<label for="country">Permis</label>
				<input 
					type="text" 
					class="form-control" 
					id="local" 
					name="permis"
					placeholder="Permis ...."
				/>
			
				<label for="desc">Date de Permis</label>
				<input 
					type="date" 
					class="form-control" 
					id="desc" 
					name="DatePirmis"
					placeholder="Date de Permis "
				/>
				<label for="departements">Ville</label>
				<input 
					type="text" 
					class="form-control" 
					id="departements.name" 
					name="ville"
					placeholder="Ville .."
				/>
				<label for="country">Téléphone</label>
				<input 
					type="number" 
					class="form-control" 
					id="local" 
					name="Phone"
					placeholder="votre phone"
				/>
			<button type="submit" class="btn btn-info">Sauvegarder</button>
		</form >
